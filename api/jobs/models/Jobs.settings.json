{
  "connection": "default",
  "collectionName": "jobs",
  "info": {
    "name": "jobs",
    "description": "latest jobs"
  },
  "options": {
    "timestamps": true
  },
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "image": {
      "model": "file",
      "via": "related",
      "plugin": "upload"
    },
    "video": {
      "type": "string"
    },
    "content": {
      "type": "text"
    },
    "salary": {
      "type": "string"
    },
    "worktypes": {
      "model": "worktypes"
    },
    "user": {
      "model": "user",
      "via": "jobs",
      "plugin": "users-permissions"
    },
    "employer": {
      "model": "employers"
    },
    "joblocation": {
      "model": "joblocation"
    }
  }
}